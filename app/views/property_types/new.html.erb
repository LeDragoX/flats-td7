<header>
    <h1>Cadastro de Tipo de Im√≥vel</h1>

    <nav>
        <%= link_to('Voltar', root_path) %>
        <hr>
    </nav>
</header>
<main>
    <%= form_with(model: @property_type) do |f| %>
        <div>
            <%= f.label :name %>
            <%= f.text_field :name %>
            <% @property_type.errors.full_messages_for(:name).each do |message| %>
                <div class="form-error"><%= message %></div>
            <% end %>
        </div>
        <%= f.submit "Enviar" %>
    <% end %>
</main>